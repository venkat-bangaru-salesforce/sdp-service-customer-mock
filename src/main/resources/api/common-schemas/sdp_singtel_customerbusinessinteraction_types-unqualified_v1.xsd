        </xs:complexType>
      </xs:element>
      <xs:element name="closeSummaries" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The statistics calculated for the CustomerProblemTask each time it has been closed.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="closeSummarie" type="tns:CloseCustomerProblemSummary" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="creationDate" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The date and time the entity instance was created. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="description" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Long free-format description of the task.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="1023"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="elapsedTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Elapsed time before completion time that a warning will be generated for the task completion.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:duration">
            <xs:pattern value="(\-)?P\p{Nd}{1,4}D(T\p{Nd}{1,4}H\p{Nd}{1,4}M\p{Nd}{1,4}(\.\p{Nd}{1,3})?S)?"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="id" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A unique identifier. </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="lastModificationDate" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The date and time the entity instance was last modified. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="name" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Short free format textual description of the task.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="80"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="priority" type="tns:CustomerProblemTaskPriority" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Defines the importance of the task. The priority usually influences business rules and in selecting the entitlement to
support.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="problemDetails" type="tns:CustomerProblem" minOccurs="0">
        <xs:annotation>
          <xs:documentation></xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="requiredDate" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Date and time that the task must be completed.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="severity" type="tns:CustomerProblemTaskSeverity" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Severity of the task.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="status" type="tns:CustomerProblemTaskStatus" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Current status of the task, when status is changed a ChangeStatusLog will be created.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="type" type="tns:CustomerProblemTaskType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>General or administrative. Administrative type is used for tasks which the parent CustomerProblem is not dependent on.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="warningTime" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Date and time of warning to be generated for the task completion.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="workItem" type="ns3:WorkItem" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The manual workflow state of the CustomerProblemTask.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

