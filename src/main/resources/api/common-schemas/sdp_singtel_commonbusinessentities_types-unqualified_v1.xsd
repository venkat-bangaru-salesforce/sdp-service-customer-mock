            <xs:annotation>
              <xs:documentation>Type.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="revenueRange" type="tns:RevenueRange" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the size of revenue.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="sizeEmployees" type="tns:SizeEmployees" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates the number of employees.</xs:documentation>
