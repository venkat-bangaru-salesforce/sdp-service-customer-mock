  <xs:complexType name="IndividualName" final="extension restriction">
    <xs:annotation>
      <xs:documentation>A phrase by which an Individual is known and distinguished from other Individuals. An IndividualName is an informal way
of identifying an Individual. The model allows only one IndividualName for an Individual. This is a restriction
compared to TMF/NGOSS SID model which allows for multiple.
:
Derived from PIM entity IndividualName in the domain party.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="familyName" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The (usually) non-chosen inherited name. Also known as last name or surname.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="formattedName" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A concatenation of all the components of the name, including optional additional components that are not separately
managed such as family generation of qualifications. Also proper punctuation will be included.
</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="givenName" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The primary chosen name. Also known as first name.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="id" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A unique identifier. </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="latinName" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The Individual name in latin script. Used when the native script is non-latin (such as Hebrew, Arabic or Japanese)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="legalName" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A concatenation of all the components of the name.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="middleName" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Additional chosen names, or their initials only.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="preferredGivenName" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The name by which an individual prefers to be addressed. It could be different from the other names, such as a nickname.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="previousName" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Previous name of this Individual</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="salutation" type="tns:Salutation" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The salutation, also known as person title.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="validityEnd" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Date on which the name ceased to be valid.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="validityStart" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Date on which the name became valid.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

