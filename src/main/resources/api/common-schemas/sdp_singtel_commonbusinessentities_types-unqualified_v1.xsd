  <xs:complexType name="Individual" final="extension restriction">
    <xs:annotation>
      <xs:documentation>A single human being, a person, with whom the service provider does business or who has business-related relevance to
the service provider.
:
Derived from PIM entity Individual in the domain party.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:Party">
        <xs:sequence>
          <xs:element name="additionalId" type="ns1:AdditionalId" minOccurs="0">
            <xs:annotation>
              <xs:documentation>An additional key for the entity. Typically used for entities that may be referred to by multiple external systems,
each of which has its own identifier. The consumer specifies one additionalId but the provider persists multiple
additionalIds.
</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="anonymous" minOccurs="0">
            <xs:annotation>
              <xs:documentation>If set to TRUE  indicates that Individual's identity is not known.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:boolean">
                <xs:pattern value="false|true"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="birthCountry" type="ns1:CountryCode" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The Country in which the Individual was born</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="birthDate" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Date on which the person was born.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="deathDate" type="xs:dateTime" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Date on which the person died.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="externalID" minOccurs="0">
            <xs:annotation>
              <xs:documentation>An Identifier allocated by the consumer when the entity is created. May not be updated.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:maxLength value="255"/>
                <xs:whiteSpace value="collapse"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="gender" type="tns:Gender" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Gender of the person.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="identifications" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The indentification documents by which an Individual identifies oneself</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="identification" type="tns:IndividualIdentification" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="language" type="ns1:LanguageCode" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The primary language of the Individual.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="maritalStatus" type="tns:MaritalStatus" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Marital status of the person.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="name" type="tns:IndividualName" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The name by which an Individual is known</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="personRank" type="xs:integer" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Degree of importance of the person to the service provider.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="IndividualIdentification" abstract="true">
