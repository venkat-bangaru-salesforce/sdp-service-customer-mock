        <xs:annotation>
          <xs:documentation>The type of value that the Characteristic can take on</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="PurposedText" final="extension restriction">
