<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema elementFormDefault="qualified" version="1.0"
           targetNamespace="http://group.singtel.com/commonbusinessentities/types-unqualified/v1" xmlns:ns2="http://group.singtel.com/customerbilling/types-unqualified/v1" xmlns:ns1="http://group.singtel.com/core/types-unqualified/v1" xmlns:ns4="http://group.singtel.com/customerproduct/types-unqualified/v1" xmlns:ns3="http://group.singtel.com/customerprofile/types-unqualified/v1" xmlns:tns="http://group.singtel.com/commonbusinessentities/types-unqualified/v1" xmlns:ns9="http://group.singtel.com/enterprise/types-unqualified/v1" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ns5="http://group.singtel.com/customerbusinessinteraction/types-unqualified/v1" xmlns:ns6="http://group.singtel.com/productcatalog/types-unqualified/v1" xmlns:ns7="http://group.singtel.com/service/types-unqualified/v1" xmlns:ns10="http://group.singtel.com/supplierpartner/types-unqualified/v1" xmlns:ns8="http://group.singtel.com/resource/types-unqualified/v1" xmlns:ns11="http://group.singtel.com/marketsales/types-unqualified/v1">

  <xs:annotation>
    <xs:documentation>The common business entities domain describes common modeling concepts across one or more parts of the Information
Model.Singtel interfaces.
</xs:documentation>
  </xs:annotation>
  <xs:import namespace="http://group.singtel.com/core/types-unqualified/v1" schemaLocation="sdp_singtel_core_types-unqualified_v1.xsd"/>

  <xs:import namespace="http://group.singtel.com/customerbilling/types-unqualified/v1" schemaLocation="sdp_singtel_customerbilling_types-unqualified_v1.xsd"/>

  <xs:import namespace="http://group.singtel.com/customerprofile/types-unqualified/v1" schemaLocation="sdp_singtel_customerprofile_types-unqualified_v1.xsd"/>

  <xs:import namespace="http://group.singtel.com/customerproduct/types-unqualified/v1" schemaLocation="sdp_singtel_customerproduct_types-unqualified_v1.xsd"/>

  <xs:import namespace="http://group.singtel.com/customerbusinessinteraction/types-unqualified/v1" schemaLocation="sdp_singtel_customerbusinessinteraction_types-unqualified_v1.xsd"/>

  <xs:import namespace="http://group.singtel.com/productcatalog/types-unqualified/v1" schemaLocation="sdp_singtel_productcatalog_types-unqualified_v1.xsd"/>

  <xs:import namespace="http://group.singtel.com/service/types-unqualified/v1" schemaLocation="sdp_singtel_service_types-unqualified_v1.xsd"/>

  <xs:import namespace="http://group.singtel.com/resource/types-unqualified/v1" schemaLocation="sdp_singtel_resource_types-unqualified_v1.xsd"/>

  <xs:import namespace="http://group.singtel.com/enterprise/types-unqualified/v1" schemaLocation="sdp_singtel_enterprise_types-unqualified_v1.xsd"/>

  <xs:import namespace="http://group.singtel.com/supplierpartner/types-unqualified/v1" schemaLocation="sdp_singtel_supplierpartner_types-unqualified_v1.xsd"/>

  <xs:import namespace="http://group.singtel.com/marketsales/types-unqualified/v1" schemaLocation="sdp_singtel_marketsales_types-unqualified_v1.xsd"/>

  <xs:complexType name="ActivityLog" abstract="true">
    <xs:annotation>
      <xs:documentation>Base type for business interaction activities such as research, phone conversations and on site activities which are
recorded as log entries against the business interaction.

The ActivityLog records the application ApplicationUser which created the log record and if applicable the Employee
which was the source of the Log information. For example the Employee may be a site engineer who does not have access
to the application and a ApplicationUser of the application creates the log for them.
:
Derived from PIM entity ActivityLog in the domain logs.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="activityNotes" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Notes describing the activity that occurred.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="creationDate" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Date the activity was performed.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="id" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A unique identifier. </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="originatingEmployee" type="tns:Employee" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The Employee who was the source of the entry. The Employee may not be the ApplicationUser of the application.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="originatingUser" type="tns:ApplicationUser" minOccurs="0">
        <xs:annotation>
          <xs:documentation>ApplicationUser who performed the activity.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="type" type="tns:ActivityType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Records the type of activity that was performed.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Employee" final="extension restriction">
    <xs:annotation>
      <xs:documentation>A role that an Individual plays for compensation to an employer and whose duties are under the control of the employer.
An Employee is located at an InternalSite and has its working hours defined by one or more EmployeeWorkingHours.

Note that the model has no association between Employee and Individual due to the lack of support in current
applications.
:
Derived from PIM entity Employee in the domain employee.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="id" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A unique identifier. </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="locatedAt" type="tns:InternalSite" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The internalSite where the employee is located.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="reports" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The Employees reporting to the supervisor.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="report" type="tns:Employee" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="supervisor" type="tns:Employee" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The supervisor of the Employee.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="title" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The employee's job title.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="workHours" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The working hours of the Employee. Only one set of working hours is currently effective.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="workingHour" type="tns:EmployeeWorkingHours" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="InternalSite" final="extension restriction">
    <xs:annotation>
      <xs:documentation>This is the GeographicSite where the Employee physically works. Used to determine the regional information including
time zone of the employee for notification purposes.
:
Derived from PIM entity InternalSite in the domain employee.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:GeographicSite">
        <xs:sequence>
          <xs:element name="employees" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The employees located at the internalsite. employees can derive their work hours, timezone etc from where they are
located.
</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="employee" type="tns:Employee" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="storeType" type="tns:StoreType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Type of store located at the InternalSite.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="GeographicSite" abstract="true">
    <xs:annotation>
      <xs:documentation>Portion of the earth's surface, the air above it or the soil beneath it, that is of interest to the service provider.
:
Derived from PIM entity GeographicSite in the domain location.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ceaseDate" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Date on which the GeographicSite is no longer of interest.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="compartments" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The inner compartments, which are also GeographicSites,  that are included in this GeographicSite, i.e. those that
compose it.
</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="compartment" type="tns:GeographicSite" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="description" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Free format textual description.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="1023"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="establishDate" type="xs:dateTime" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Date on which the GeographicSite was established, i.e. became of interest.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="externalID" minOccurs="0">
        <xs:annotation>
          <xs:documentation>An Identifier allocated by the consumer when the entity is created. May not be updated.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="id" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A unique identifier. </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="locatedAt" type="tns:GeographicAddress" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The complete or portion of the GeographicAddress at which this GeographicSite is located.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="name" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Free format short textual description. A name is an informal way of identifying a GeographicSite.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="owningOrganization" type="tns:Organization" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The Organization which owns the site.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="parent" type="tns:GeographicSite" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The parent GeographicSite.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="type" type="tns:GeographicSiteType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The physical structural nature of the GeographicSite.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="StoreType" final="extension restriction">
    <xs:annotation>
      <xs:documentation>The type of business store present at the GeographicLocation. The store type is only relevant for certain sub-types of
GeographicLocation. There is no effort to differentiate the store types available for these sub-types. For example a
call center may be present at an InternalSite or a retail store may be present at a VendorSite.
</xs:documentation>
    </xs:annotation>
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="GeographicAddress" final="extension restriction">
    <xs:annotation>
      <xs:documentation>A structured textual way of describing where a GeographicSite is. It is usually composed of an ordered list of names of
geographic locations according to rules governed by context.
:
Derived from PIM entity GeographicAddress in the domain location.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="altitude" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The vertical distance of Above Ground Level (AGL) or from Mean Sea Level (MSL), e.g. 1500 m, 1800 ft.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="1023"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="apartment" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A reference to identify a single living compartment in a house, usually used by a single family.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="buildingName" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A reference to identify a well known building usually office building, e.g. The White House.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="city" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A reference to identify a municipality in which the street or building is located.
Conceptually, it should have been of type AdministrativeArea, and have a containment relation to it. However, for
practical reasons of volume of data, AdministrativeArea objects will not be instantiated and persisted thus the type of
string will prevent run-time failures.
</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="country" type="ns1:CountryCode" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The Country to which the GeographicAddress refers.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="county" type="tns:County" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The County to which the GeographicAddress refers.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="floor" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A reference to identify a single horizontal, usually flat, surface in a multi-surfaced building.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="formattedAddress1" minOccurs="0">
        <xs:annotation>
          <xs:documentation>First line of the concatenated address information as fit for location usage.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="1023"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="formattedAddress2" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Second line of the concatenated address information as fit for location usage.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="1023"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="formattedAddress3" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Third line of the concatenated address information as fit for location usage.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="1023"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="formattedAddress4" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Fourth line of the concatenated address information as fit for location usage.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="1023"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="id" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A unique identifier. </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="latitude" type="xs:decimal" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Angular distance, in degrees, minutes, and seconds of a point north or south of the Equator. The representation uses
degrees and fractions of degrees. Degrees are be expressed as a decimal fraction: 23.45833. The convention  that North
be positive is used.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="longitude" type="xs:decimal" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The east-west geographic coordinate measurement in degrees, minutes, and seconds. The representation uses degrees and
fractions of degrees. Degrees are be expressed as a decimal fraction: 23.45833. The convention  that East be positive
is used.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="postalCode" type="tns:GeographicPostalAreaCode" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The GeographicPostalArea in which the GeographicalAddress is located.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="room" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A reference to identify a single compartment inside an apartment.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="state" type="tns:State" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The State  to which the GeographicAddress refers.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="street" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A reference to identify a geographic path along which buildings stand. Conceptually, it should have been of type
AdministrativeArea, and have a containment relation to it. However, for practical reasons of volume of data,
AdministrativeArea objects will not be instantiated and persisted thus the type of string will prevent run-time
failures.
</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="streetNumber" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A reference to identify a single building in a street.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="1023"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="timezone" type="ns1:TimeZoneId" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The timezone that the address resides in.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="town" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A reference to identify a small municipality in which the street or building is located.
Conceptually, it should have been of type AdministrativeArea, and have a containment relation to it. However, for
practical reasons of volume of data, AdministrativeArea objects will not be instantiated and persisted thus the type of
string will prevent run-time failures.
</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="County" final="extension restriction">
    <xs:annotation>
      <xs:documentation>Particular places of portions of states or provinces in countries which can be located on the globe, e.g., Orange
county in California state in the USA.
:
Derived from PIM entity County in the domain location.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="id" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A unique identifier. </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="name" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Free format short textual description. A name is an informal way of identifying a County.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="timeZone" type="ns1:TimeZoneId" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The TimeZone in which the County is located</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="GeographicPostalAreaCode">
    <xs:annotation>
      <xs:documentation>Particular places of postal areas (commonly known as postal codes or zip codes) in countries which can be located on
the globe.
Derived from PIM entity GeographicPostalArea in the domain location.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="19"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="State" final="extension restriction">
    <xs:annotation>
      <xs:documentation>Particular geographic place, within a Country, which can be located on the globe, e.g., California in the United States
of America, Victoria in Canada.
:
Derived from PIM entity State in the domain location.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="code" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A value used as unique identification within a Country, e.g., CA in the United States of America, QC in Canada.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="1023"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="counties" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The Countys which are included in the State</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="countie" type="tns:County" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="id" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A unique identifier. </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="name" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Free format short textual description. A name is an informal way of identifying a State.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
