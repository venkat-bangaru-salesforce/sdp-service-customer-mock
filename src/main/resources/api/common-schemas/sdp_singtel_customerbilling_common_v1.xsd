<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema elementFormDefault="qualified" version="1.0" 
	targetNamespace="http://group.singtel.com/customerbilling-common/types/v1" 
	xmlns:tns="http://group.singtel.com/customerbilling-common/types/v1"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:ns1="http://group.singtel.com/core/types/v1"
	xmlns:ns2="http://group.singtel.com/customerprofile-common/types/v1"
	xmlns:ns3="http://group.singtel.com/commonbusinessentities/types-unqualified/v1">

	<xs:annotation>
		<xs:documentation>Shared types from the customer billing domain which models the information required for the production of the
			CustomerBills and the payment or non-payment of those CustomerBills.Singtel interfaces.
		</xs:documentation>
	</xs:annotation>
	
	<xs:import namespace="http://group.singtel.com/core/types/v1" schemaLocation="sdp_singtel_core_v1.xsd"/>
	<xs:import namespace="http://group.singtel.com/customerprofile-common/types/v1" schemaLocation="sdp_singtel_customerprofile_common_v1.xsd"/>
	<xs:import namespace="http://group.singtel.com/commonbusinessentities/types-unqualified/v1" schemaLocation="sdp_singtel_commonbusinessentities_types-unqualified_v1.xsd"/>
	
	<xs:complexType name="AbstractCustomerInvoiceID" abstract="true">
		<xs:annotation>
			<xs:documentation>An abstract base type for different types of invoices such as CustomerInvoice and CustomerCreditNote
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}AbstractCustomerInvoice .
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tns:FinancialTransactionID">
				<xs:sequence>
					<xs:element name="id" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The Billing internal identifier of the Invoice.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="255" />
								<xs:whiteSpace value="collapse" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="FinancialTransactionID" abstract="true">
		<xs:annotation>
			<xs:documentation>A transaction on a CustomerAccount. Transaction stands for every financial activity performed for the CustomerAccount
				and impacts its balance. The Billing internal identifier of the transaction.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}FinancialTransaction .
			</xs:documentation>
		</xs:annotation>
		<xs:sequence />
	</xs:complexType>

	<xs:complexType name="CustomerInvoiceID" final="extension restriction">
		<xs:annotation>
			<xs:documentation>A Business Context Model Identifier.:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}CustomerInvoice .
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tns:AbstractCustomerInvoiceID">
				<xs:sequence />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="AbstractCustomerInvoiceNumber" abstract="true">
		<xs:annotation>
			<xs:documentation>An abstract base type for different types of invoices such as CustomerInvoice and CustomerCreditNote
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}AbstractCustomerInvoice .
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tns:FinancialTransactionNumber">
				<xs:sequence>
					<xs:element name="invoiceNumber">
						<xs:annotation>
							<xs:documentation>The Billing Legal invoice number.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="255" />
								<xs:whiteSpace value="collapse" />
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="FinancialTransactionNumber" abstract="true">
		<xs:annotation>
			<xs:documentation>A transaction on a CustomerAccount. Transaction stands for every financial activity performed for the CustomerAccount
				and impacts its balance. The Billing number of the transaction.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}FinancialTransaction .
			</xs:documentation>
		</xs:annotation>
		<xs:sequence />
	</xs:complexType>

	<xs:complexType name="CustomerInvoiceNumber" final="extension restriction">
		<xs:annotation>
			<xs:documentation>A Business Context Model Identifier.:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}CustomerInvoice .
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tns:AbstractCustomerInvoiceNumber">
				<xs:sequence />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="AppliedCustomerBillingRateCoreID" final="extension restriction">
		<xs:annotation>
			<xs:documentation>Concrete type for AppliedCustomerBillingRate.
				:
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tns:AppliedCustomerBillingRateID">
				<xs:sequence />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="AppliedCustomerBillingRateID" abstract="true">
		<xs:annotation>
			<xs:documentation>A debit or a credit calculated for to the CustomerAccount, as part of the rating process, as part of the billing
				process, or as a result of one time activities performed for the CustomerAccount such as rate applied due to duplicate
				bill request.
				:
				Derived from PIM entity AppliedCustomerBillingRate in the domain appliedcustomerbillingrate.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id">
				<xs:annotation>
					<xs:documentation>An identifier along with the AppliedCustomerBillingRate.SubID forms the unique identification. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="AssignedBillingOfferID" final="extension restriction">
		<xs:annotation>
			<xs:documentation>The pricing agreement, guiding and other rating related information of a Product that is associated with the
				Subscription. The AssignedBillingOffer includes the price plan, and their related offers (if any exist) for the
				Subscription.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}AssignedBillingOffer .
				Derived from PIM entity AssignedBillingOffer in the domain customerpriceagreement.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id">
				<xs:annotation>
					<xs:documentation>The Billing internal identifier of the assigned offer.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ChargeCode" final="extension restriction">
		<xs:annotation>
			<xs:documentation>A charge code assigned to a AppliedCustomerBillingRate.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}ChargeCode .
				Derived from PIM entity ChargeCode in the domain appliedcustomerbillingrate.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="code">
				<xs:annotation>
					<xs:documentation>The Billing code for the charge.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustomerBillID" final="extension restriction">
		<xs:annotation>
			<xs:documentation>A presentation of all AppliedCustomerBillingRates and financial activities made during the defined billing cycle period,
				and record of the amount due of the CustomerBillSpec.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}CustomerBill .
				Derived from PIM entity CustomerBill in the domain customerbill.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id">
				<xs:annotation>
					<xs:documentation>The Billing document identifier of the CustomerBill.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustomerBillingCycleSpecificationCode" final="extension restriction">
		<xs:annotation>
			<xs:documentation>The cycles in the system. CustomerBillingCycleSpecification are instantiated as CustomerBillingCycle for each
				billing period. For example, if the service provider runs a single cycle every day and all the cycles in the system are
				monthly cycles then there will be 30 instances of CustomerBillingCycleSpecification in the billing system.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}CustomerBillingCycleSpecification .
				Derived from PIM entity CustomerBillingCycleSpecification in the domain customerbill.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cycleCode" type="xs:decimal">
				<xs:annotation>
					<xs:documentation>The unique identifier of the cycle specification. </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="PayChannelID" final="extension restriction">
		<xs:annotation>
			<xs:documentation>The PayChannel class is used to abstract the way a customer pays for ongoing (not immediate) charges. It can either be
				through a PayInstruction or PrepaidBalance but not both, hence the associations from PayChannel are mutually exclusive.
				This class models the event/charge distribution instruction and not how a specific immediate charge was paid.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerprofile/types-unqualified/v1}PayChannel .
				Derived from PIM entity PayChannel in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The ID of the PayChannel - this can be either ID of PrepaidBalance or ID of PayInstruction as denoted by the value of
						{PayChannel.idType.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="PaymentDetailsID" final="extension restriction">
		<xs:annotation>
			<xs:documentation>The identifier of the Payment, for example cheque , credit card.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}PaymentDetails .
				Derived from PIM entity PaymentDetails in the domain customeraccountactivities.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id">
				<xs:annotation>
					<xs:documentation>The Billing internal identifier of the PaymentDetails.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SubscriptionGroupID" final="extension restriction">
		<xs:annotation>
			<xs:documentation>Groups one or more SubscriptionGroups or Subscriptions in a way that allows to model a complex customer structure. For
				example, in a family customer the subscription group may group together the subscription members of the family. In a
				corporate example, there may be many subscription groups representing the corporate structure to units and divisions.
				The leaves of the hierarchal tree are always Subscriptions.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}SubscriptionGroup,
				Derived from PIM entity SubscriptionGroup in the
				domain customerpriceagreement.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A unique identifier. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SubscriptionID" final="extension restriction">
		<xs:annotation>
			<xs:documentation>A Business Context Model Identifier.:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}Subscription .
				Derived from PIM entity Subscription in the domain customerpriceagreement.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id">
				<xs:annotation>
					<xs:documentation>The Billing internal identifier of the Subscription.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="ChargeOrigin">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="AR">
				<xs:annotation>
					<xs:documentation>Charge received from AR</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BACKDATING">
				<xs:annotation>
					<xs:documentation>Charge inserted by BTLEOD as part of backdated transactions processing</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BOD_COMPENSATION">
				<xs:annotation>
					<xs:documentation>Bill on Demand compensation charge</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CHARGE_CALCULATOR">
				<xs:annotation>
					<xs:documentation>Charge calculator</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CM">
				<xs:annotation>
					<xs:documentation>Charge received from CM</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DIFF_CALCULATOR">
				<xs:annotation>
					<xs:documentation>Diff calculator</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DISCOUNT">
				<xs:annotation>
					<xs:documentation>Discount</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DOCUMENTATION">
				<xs:annotation>
					<xs:documentation>Documentation</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INVOICING">
				<xs:annotation>
					<xs:documentation>Invoicing</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MANUAL">
				<xs:annotation>
					<xs:documentation>Manually created charge</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="M_COMMERCE">
				<xs:annotation>
					<xs:documentation>M-commerce</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="QUOTATION_SERVER">
				<xs:annotation>
					<xs:documentation>Charge Inserted by Quotation Server</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RATER">
				<xs:annotation>
					<xs:documentation>Charge received from Rater</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CollectionCategory">
		<xs:annotation>
			<xs:documentation>Collection categories define the conditions to start collections treatment for CustomerAccounts, and the collections
				actions to be taken (i.e., the CollectionPolicy to be used). For example, all residential customers whose past due
				amount is greater than $100 are subjected to the collection treatment.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}CollectionCategory .
				Derived from PIM entity CollectionCategory in the domain customercollections.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ALL">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INCOMING">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INTERNATIONAL">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OUTGOING">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CollectionPolicy">
		<xs:annotation>
			<xs:documentation>Collection policy codes available in the system to be used by Collections. Collection policies determine the list of
				activities to be performed for CustomerAccounts in collection treatment, including the time interval between the
				activities, and the conditions to be met to perform the activity.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}CollectionPolicy .
				Derived from PIM entity CollectionPolicy in the domain customercollections.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string" />
	</xs:simpleType>

	<xs:simpleType name="CollectionStatus">
		<xs:annotation>
			<xs:documentation>The collection status of a CustomerAccount.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}CollectionStatus .
				Derived from PIM entity CollectionStatus in the domain customercollections.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="IN_COLLECTION">
				<xs:annotation>
					<xs:documentation>In collections treatment.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IN_TRIAL_PERIOD">
				<xs:annotation>
					<xs:documentation>In trial period.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NOT_IN_COLLECTION">
				<xs:annotation>
					<xs:documentation>Not in collections treatment.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CollectionStatusBilling">
		<xs:annotation>
			<xs:documentation>The collection status of a CustomerAccount.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}CollectionStatus .
				Derived from PIM entity CollectionStatus in the domain customercollections.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="CANCEL">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NONE">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RESTORE">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SUSPEND">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CustomerBillStatus">
		<xs:annotation>
			<xs:documentation>The available statuses of the CustomerBill.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}CustomerBillStatus .
				Derived from PIM entity CustomerBillStatus in the domain customerbill.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="C">
				<xs:annotation>
					<xs:documentation>Deprecated - use CALCULATED instead</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CALCULATED">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CONFIRMED">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation>Deprecated - use CONFIRMED instead</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="PaymentMethod">
		<xs:annotation>
			<xs:documentation>The method by which a Payment is made.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}PaymentMethod .
				Derived from PIM entity PaymentMethod in the domain customeraccountactivities.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="CASH">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CHEQUE">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CREDIT_CARD">
				<xs:annotation>
					<xs:documentation>Used also for debit card payment</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DD_DIRECT_DEBIT">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OTHER">
		        <xs:annotation>
		          <xs:documentation>Other payment method</xs:documentation>
		        </xs:annotation>
	      	</xs:enumeration>
	      	<xs:enumeration value="DASH">
		        <xs:annotation>
		          <xs:documentation>Dash payment method</xs:documentation>
		        </xs:annotation>
		      </xs:enumeration>
		      <xs:enumeration value="ALL">
		        <xs:annotation>
		          <xs:documentation>Used to search all payments</xs:documentation>
		        </xs:annotation>
		      </xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ProductionType">
		<xs:annotation>
			<xs:documentation>The type of a CustomerBill.
				:
				Restriction of {http://amdocs/pbg-portfolio/customerbilling/types-unqualified/v1}ProductionType .
				Derived from PIM entity ProductionType in the domain customerbill.
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="DROP">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FINAL_BILL">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FIRST_BILL">
				<xs:annotation>
					<xs:documentation></xs:documentation>
