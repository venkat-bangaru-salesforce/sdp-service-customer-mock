  <xs:complexType name="County" final="extension restriction">
    <xs:annotation>
      <xs:documentation>Particular places of portions of states or provinces in countries which can be located on the globe, e.g., Orange
county in California state in the USA.
:
Restriction of {http://amdocs/pbg-portfolio/commonbusinessentities/types-unqualified/v1}County .
Derived from PIM entity County in the domain location.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="id" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A unique identifier. </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="name" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Free format short textual description. A name is an informal way of identifying a County.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="127"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="timeZone" type="ns1:TimeZoneId" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The TimeZone in which the County is located</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

