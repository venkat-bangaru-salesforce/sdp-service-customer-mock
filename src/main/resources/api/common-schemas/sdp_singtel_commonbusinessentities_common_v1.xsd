  <xs:complexType name="FormattedGeographicAddress" final="extension restriction">
    <xs:annotation>
      <xs:documentation>A structured textual way of describing where a Geographic Site is. It is usually composed of an ordered list of names
of geographic locations according to rules governed by context.
:
Restriction of {http://amdocs/pbg-portfolio/commonbusinessentities/types-unqualified/v1}GeographicAddress .
Derived from PIM entity GeographicAddress in the domain location.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="formattedAddress1" minOccurs="0">
        <xs:annotation>
          <xs:documentation>First line of the concatenated address information as fit for location usage.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="1023"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="formattedAddress2" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Second line of the concatenated address information as fit for location usage.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="1023"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="formattedAddress3" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Third line of the concatenated address information as fit for location usage.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="1023"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="formattedAddress4" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Fourth line of the concatenated address information as fit for location usage.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="1023"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="id">
        <xs:annotation>
          <xs:documentation>The CRM internal identifier of the address.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="255"/>
            <xs:whiteSpace value="collapse"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

