        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="EXPIRED">
        <xs:annotation>
          <xs:documentation>The ProductOrder expiration date has passed.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="FUTURE">
        <xs:annotation>
          <xs:documentation>All the ProductOrderItems are in future status.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="INITIAL">
        <xs:annotation>
          <xs:documentation>The ProductOrder was created but processing of its ProductOrderItems has not started yet.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="OPEN">
        <xs:annotation>
          <xs:documentation>Some or all of the ProductOrderItems are still in progress.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="REJECT">
        <xs:annotation>
          <xs:documentation>At least one of the ProductOrderItems is in a reject status</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SUBMITTED">
        <xs:annotation>
          <xs:documentation>Some or all of the ProductOrderItems are still in progress, but all have been submitted for delivery.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SUBMIT_REQUESTED">
        <xs:annotation>
