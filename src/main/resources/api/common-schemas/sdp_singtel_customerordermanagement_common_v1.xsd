  <xs:simpleType name="ProductOrderStatus">
    <xs:annotation>
      <xs:documentation>The status of a Product Order as a whole, typically calculated from the status of the contained Product Order Items.
:
Restriction of {http://amdocs/pbg-portfolio/customerbusinessinteraction/types-unqualified/v1}ProductOrderStatus .
Derived from PIM entity ProductOrderStatus in the domain customerorder.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="CANCELED">
        <xs:annotation>
          <xs:documentation>All the Product Order Item are canceled.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CLOSED">
        <xs:annotation>
          <xs:documentation>All the ProductOrderItems are completed.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DISCONTINUED">
        <xs:annotation>
