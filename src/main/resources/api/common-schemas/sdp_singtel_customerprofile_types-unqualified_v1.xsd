	<xs:complexType name="CustomerAccountTaxExemption" final="extension restriction">
		<xs:annotation>
			<xs:documentation>The freedom from taxes imposed by a taxing authority.
				:
				Derived from PIM entity CustomerAccountTaxExemption in the domain
				customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="certificateNumberr" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The identifier, for example number of reference, of the tax exemption certificate as received from the tax authority.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A unique identifier. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="taxAuthority" type="ns2:TaxAuthority" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The tax authority that granted the exemption and issued the certificate.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxAuthorityType" type="ns2:TaxAuthorityType" minOccurs="0">
