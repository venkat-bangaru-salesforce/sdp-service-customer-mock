	 <xs:complexType name="UpdatePINsForDevice" final="extension restriction">
	   <xs:annotation>
	     <xs:documentation>The request dataType for managePinService.updatePINsForDevice operation, this structure include all the fields that are part of the request.
		This operation accepts a list of deviceExternalIDs, where all IDs shares the same input information for updating PINs. 
		Internally SDP invokes the remote system (MMIG) multiple times depending on the number of deviceExternalIDs. 
		As long as an invocation fails, error will be returned and caller has to re-invoke UpdatePINsForDevice.
		</xs:documentation>
	   </xs:annotation>
	   <xs:sequence>
	   	<xs:element name="serviceNumber" type="xs:string" />
	   	<xs:element name="deviceExternalIDs" type="tns:DeviceExternalIDs"/>
	   	<xs:element name="pinModifiedDateTime" type="xs:dateTime"/>
	   	<xs:element name="pinDetails" type="tns:PINDetails"/>
	   </xs:sequence>
	 </xs:complexType>
	 
	 <xs:complexType name="UpdatePINsForDeviceResponse" final="extension restriction">
	   <xs:annotation>
	     <xs:documentation>No response other than the basic response defined in the document D0025-9000 – SDP HLSD (Section 4) is required by this operation.</xs:documentation>
	   </xs:annotation>
	   <xs:complexContent>
			<xs:extension base="sgc:SDPResponse">
			</xs:extension>
		</xs:complexContent>		
	 </xs:complexType>
  
  	<xs:complexType name="DeviceExternalIDs">
	  	<xs:annotation>
	  		<xs:documentation>One or more device ID used by external systems, e.g. MMIG.
				This value(s) must be provided by caller.
				For BCC, if this ID is not stored in calling system, it would be mapped in MEC.</xs:documentation>
	  	</xs:annotation>
	  	<xs:sequence>
	  		<xs:element name="deviceExternalID" type="xs:string" maxOccurs="unbounded" />
	  	</xs:sequence>
  	</xs:complexType>

	<xs:complexType name="PINDetails">
	  	<xs:annotation>
	  		<xs:documentation>Data type containing PIN information.</xs:documentation>
