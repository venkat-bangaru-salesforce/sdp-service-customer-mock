	<xs:import schemaLocation="../common-schemas/sdp_singtel_service_common.xsd" namespace="http://group.singtel.com/enterpriseapplicationintegration/common-types/v1"></xs:import>

	<xs:complexType name="PayInstruction" final="extension restriction">
		<xs:annotation>
			<xs:documentation>A standing order for automatic collection of payments (in order to pay bills - for example) via electronic fund
				transfer.
				:
				Derived from PIM entity PayInstruction in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="amount" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The amount of the payment to be automatically collected. The amount must be specified for prepay payments. It is
						inapplicable for payment of postpaid bills, it will not be specified thus causing the bill amount to be collected.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bankAccount" type="tns:BankAccount" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The BankAccount which is used to automatically collect payments for the CustomerAccount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="card" type="tns:Card" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The Card which is used to automatically collect payments for the CustomerAccount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Visual description of the PayInstruction to be used for selecting PayInstruction or for bill display.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="1023" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="externalID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An Identifier allocated by the consumer when the entity is created. May not be updated.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="frequencyUnit" type="tns:FrequencyUnit" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The unit of measure for the intervals between auto collection of payments, for example day, week, month. It must be
						specified for prepay payments. It is inapplicable for payment of postpaid bills as payment terms are determined by the
						service
						provider.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The ID of the pay channel that pays for this PayInstruction. Uniquely identifies the PayInstruction.
						A pay channel is
						a specified payment method to which subscriber services can be mapped. A pay channel can be prepaid or
						postpaid.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="intervalBoundary" type="tns:IntervalBoundary" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The boundary on which the interval begins, for example the third day in the week, the 10th day of the month.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="intervalSize" type="tns:IntervalSize" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The quantity of unit of measure for the intervals between automatic collection of payments, for example 10 (days), 2
						(weeks), 1 (month). It must be specified for prepay payments. It is inapplicable for payment of postpaid bills as
						payment terms are
						determined by the service.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pin" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The personal identification number (PIN), which is required when using the payment method (e.g., Card or BankAccount
						associated with this PayInstruction for a one-time payment. This is instead of providing all payment method details
						again. It is
						usually relevant for PayInstruction (postpaid), but can also be used for PrepaidBalance
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="type" type="tns:PayInstructionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The PaymentType for this PayInstruction, can be one of Recurring(R), Returning(T), None(N)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validityEnd" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date on which the instruction ceases to be valid.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validityStart" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date on which the instruction became valid.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="BankAccount" final="extension restriction">
		<xs:annotation>
			<xs:documentation>A means (an instrument) for taking payments. It is usually a checking or savings account, with a financial institution,
				usually a bank, from which money can be withdrawn.
				:
				Derived from PIM entity BankAccount in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="authorizations" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Authorizations for use of the BankAccount.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="authorization" type="tns:BankAccountAuthorization" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="bankCode" type="tns:BankCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The code of the financial institution, usually as assigned by the regulator.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bankName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The name of the financial institution.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="127" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="branchCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The code of the particular office of the financial institution which manages the bank account.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A unique identifier. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="name" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The name of the bank account as maintained with the financial institution.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="127" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="number" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The bank account number as given by the financial institution. This field is encrypted.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="visibleNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A portion of the bank account number (as given by the financial institution) which remains visible and readable for
						humans. The entire bank account number is stored encrypted.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="BankAccountAuthorization" final="extension restriction">
		<xs:annotation>
			<xs:documentation>An authorization for the use of a BankAccount between a set of dates.
				:
				Derived from PIM entity BankAccountAuthorization
				in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="authorizationDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date on which the authorization was received from the financial institution.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="authorizationReference" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The financial institution reference of the received authorization.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="authorizationStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Status of the authorization received from the financial institution which pertains to the eligibility of using the
						bank
						account.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="127" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="validityEnd" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date on which the bank account authorization ceases to be valid.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validityStart" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date on which the bank account authorization became valid.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Card" final="extension restriction">
		<xs:annotation>
			<xs:documentation>A means (an instrument) for taking payments, that is, by which money can be withdrawn from an account with a credit
				company or a bank. The card can be a credit card, debit card or charge card.
				:
				Derived from PIM entity Card in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="brand" type="tns:Brand" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The brand of the card, for example Visa, MasterCard, AMEX, Diners.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cardBillingAddress" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The billing address of card at the card issuer (used for verification purposes).</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="127" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="encryptedNumber" type="ns1:BinaryObject" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The card number as given by the issuer of the card. This field is encrypted.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A unique identifier. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="imprintedName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The name of the card holder as imprinted on the card itself.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="127" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="number" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The card number as given by the issuer of the card. This field is encrypted.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="validityMonth" type="xs:gMonth" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The month through which the card is valid for use (1 to 12), during the validity year.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validityYear" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The year through which the card is valid for use.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:gYear">
						<xs:maxInclusive value="2020" />
						<xs:minInclusive value="2000" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="visibleNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A portion of the card number (as given by the issuer of the card) which remains visible and readable for humans. The
						entire card number is stored encrypted.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustomerBillSpec" abstract="true">
		<xs:annotation>
			<xs:documentation>The specification for a periodical document that is rendered to a Customer. The document lists charges and usually
				requires remittance.
				:
				Derived from PIM entity CustomerBillSpec in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="billFormat" type="ns2:CompoundBillFormat" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The format of the CustomerBills that are produced for the CustomerBillSpec.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="billProductionFrequency" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The frequency of the bill production in the units of the frequency attribute in CustomerBillingCycleSpecification.
						Valid values are:
						less-than ol greater-than
						1-12 for Monthly frequency
						1-52 for Weekly frequency
						less-than /ol greater-than
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="billedID" type="ns3:Billed" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The PartyRole that represent the Party which recieved the bill (the party that should pay for this bill).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="businessOrganizationUnit" type="ns3:BusinessOrganizationUnit" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The BusinessOrganizationUnit associated to the CustomerBillSpec, sometimes referred to as the Market. Typically
						determines business policies in business systems.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dueDateShift" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The offset used for calculating the billing due date. The billing due date is calculated as the billing date plus
						this
						number of days. The billing due date is a calendar date that specifies when all moneys must be received to promptly
						satisfy
						customer obligations for payment.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:duration">
						<xs:pattern value="(\-)?P\p{Nd}{1,4}D(T\p{Nd}{1,4}H\p{Nd}{1,4}M\p{Nd}{1,4}(\.\p{Nd}{1,3})?S)?" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="externalID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An Identifier allocated by the consumer when the entity is created. May not be updated.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A unique identifier. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="multiplier" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The frequency multiplier used for defining the cycle periods. The combination of the cycle frequency and the cycle
						multiplier defines the cycle period. For example, a cycle with Monthly frequency, and multiplier of 1 will run once a
						month. The
						multiplier is defined at the cycle level and can be overridden at the CustomerBillSpec level.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Short text, description of the CustomerBillSpec.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="127" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="openDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date on which the specification was established.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="partyRoleInvolvements" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The PartyRoles involved in the CustomerBillSpec.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="partyRoleInvolvement" type="tns:PartyRoleCustomerBillSpecInvolvement" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="payInstruction" type="tns:PayInstruction" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The PayInstruction that creates automatic postpaid payment for this CustomerBillSpec. The PayInstruction covers the
						amount by generating an automatic payment request against the associated pay means (Card or BankAccount).
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="prepaidBalances" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The PrepaidBalances that are represented on this CustomerBillSpec. These balances will appear on the corresponding
						bill
						documents.
					</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="prepaidBalance" type="tns:PrepaidBalance" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="produceDocument" type="tns:ProduceDocument" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Whether a prepaid statement should be produced or extracted.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="tns:BillSpecStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The status of the CustomerBillSpec.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxPresentment" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If value is TRUE tax will be calculated tax on each charge separately.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:boolean">
						<xs:pattern value="false|true" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="zeroBalance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether to zero the balance of the CustomerBillSpec at the billing run, for example in the case of internal
						employees.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:boolean">
						<xs:pattern value="false|true" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="PartyRoleCustomerBillSpecInvolvement" final="extension restriction">
		<xs:annotation>
			<xs:documentation>The role that a PartyRole plays for a CustomerBillSpec. This data type is an Association Role entity. It defines the
				attributes for an Association.
				:
				Derived from PIM entity PartyRoleCustomerBillSpecInvolvement in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="endDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date that the role ended.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A unique identifier. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="partyRole" type="ns3:PartyRole" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The PartyRole which has an involvement.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="role" type="tns:PartyRoleCustomerBillSpecInvolvementType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The role played.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="startDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date that the role started.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="PrepaidBalance" final="extension restriction">
		<xs:annotation>
			<xs:documentation>A monetary amount that is used to pay for usage and other charges that acrue from a pre-paid Product. Typically:

				The
				customer tops up the balance from time to time, either by an automatic PayInstruction or manually (e.g. by making
				payments at a retail
				outlet, at the call center, etc.)
				The charges are debited from the balance in real-time as usage is made of the Product.
				Usage of the
				product will be blocked in real-time if the amount decreases to zero


				:
				Derived from PIM entity PrepaidBalance in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="amount" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The current monetary amount that is available for the PrepaidBalance</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="balanceDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date on which the balance was last updated.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="balanceValidityEndDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date on which the balance is set to expire.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="businessOrganizationUnit" type="ns3:BusinessOrganizationUnit" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The BusinessOrganizationUnit associated to the PrepaidBalance, sometimes referred to as the Market. Typically
						determines business policies in business systems.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" type="ns1:CurrencyCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The currency in which the balance is calculated.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="externalID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An Identifier allocated by the consumer when the entity is created. May not be updated.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A unique identifier. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="name" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Short text, description of the PrepaidBalance.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="127" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="partyRoleInvolvements" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The PartyRoles involved in the PrepaidBalance.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="partyRoleInvolvement" type="tns:PartyRolePrepaidBalanceInvolvement" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="payInstruction" type="tns:PayInstruction" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The PayInstruction that creates automatic periodic top-up payment for this PrepaidBalance. If empty, there is no
						automatic top-up.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="pin" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The personal identification number (PIN), which is required when using the payment method (e.g., Card or BankAccount
						associated with this PayInstruction for a one-time payment. This is instead of providing all payment method details
						again. It is
						usually relevant for PayInstruction (postpaid), but can also be used for PrepaidBalance
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="recharges" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The recharges made on the PrepaidBalance.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="recharge" type="tns:Recharge" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="status" type="tns:PrepaidBalanceStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The business state of the PrepaidBalance. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="statusDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date that the PrepaidBalance.status was last modified. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="topupThreshold" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The threshold value of the PrepaidBalance for automatic top up check</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="PartyRolePrepaidBalanceInvolvement" final="extension restriction">
		<xs:annotation>
			<xs:documentation>The role that a PartyRole plays for a PrepaidBalance. This data type is an Association Role entity. It defines the
				attributes for an Association.
				:
				Derived from PIM entity PartyRolePrepaidBalanceInvolvement in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="endDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date that the role ended.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A unique identifier. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="partyRoleDetails" type="ns3:PartyRole" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The PartyRole involved in the PrepaidBalance.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="role" type="tns:PartyRolePrepaidBalanceInvolvementType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The role played.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="startDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date that the role started.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Recharge" final="extension restriction">
		<xs:annotation>
			<xs:documentation>A standing order for automatic collection of payments (in order to pay bills - for example) via electronic fund
				transfer.
				:
				Derived from PIM entity Recharge in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="amount" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The amount of the recharge applied to the PrepaidBalance.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The ID of the pay channel that pays for this PayInstruction. Uniquely identifies the PayInstruction.
						A pay channel is
						a specified payment method to which subscriber services can be mapped. A pay channel can be prepaid or
						postpaid.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="lastStatusDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date of the last status change for this Recharge</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="rechargeDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date on which the recharge happened</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="rechargeStatus" type="tns:RechargeStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The status of this Recharge</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="expiryDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This field is used to indicate expiry date.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustomerBillReceiver" final="extension restriction">
		<xs:annotation>
			<xs:documentation>The periodic document produced for CustomerBillReceiver is CustomerBill, which includes the outstanding amount due, the
				financial transactions occurred since the last document was produced, and the new AppliedCustomerBillingRates of the
				CustomerAccount.
				The
				document type is BILL_RECEIVER.
				:
				Derived from PIM entity CustomerBillReceiver in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tns:CustomerBillSpec">
				<xs:sequence>
					<xs:element name="customerAccount" type="tns:CustomerAccount" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The CustomerAccount that manages this CustomerBillReceiver</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CustomerAccount" final="extension restriction">
		<xs:annotation>
			<xs:documentation>The financial reflection of the Customer relationship. A financial bottom line, in terms of outstanding balance and
				debt amounts, of the log of financial transactions such as invoices and payments that stem from the Customer
				relationship.
				:
				Derived from
				PIM entity CustomerAccount in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ageBuckets" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The CustomerAccounts debts split by debt age.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ageBucket" type="ns2:AgeBuckets" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="balancePolicy" type="tns:BalancePolicy" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The method in which the balance is maintained.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="businessOrganizationUnit" type="ns3:BusinessOrganizationUnit" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The BusinessOrganizationUnit associated to the CustomerAccount, sometimes referred to as the Market. Typically
						determines business policies in business systems.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="collectionStatus" type="ns2:CollectionStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether the CustomerAccount is in collections treatment, not in collections treatment, or in trial period.
						Trial period is a pre-defined period of time after CustomerAccount leaves collection treatment, in which special
						handling is taken for
						activities that trigger collections re-treatment.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="collectionStatusDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date on which the collection status was most recently updated.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="collectionTreatments" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection treatments performed for the CustomerAccount when the Customer does not pay his debts on time.
					</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="collectionTreatment" type="ns2:CollectionTreatment" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="collectionWaiver" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether the CustomerAccount is waived from automatic collection process treatment.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:boolean">
						<xs:pattern value="false|true" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="collectionWaiverExpirationDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date when the automatic collections process will resume treating the CustomerAccount. Set for temporary waivers
						only.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" type="ns1:CurrencyCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The currency in which the balances are maintained.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerBillReceiver" type="tns:CustomerBillReceiver" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The CustomerBillSpec of type bill reciever associated with this CustomerAccount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerInvoiceReceivers" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The CustomerBillSpecs of type invoice reciever associated with this CustomerAccount.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="customerInvoiceReceiver" type="tns:CustomerInvoiceReceiver" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="depositBalance" type="tns:CustomerAccountBalance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The deposit balance for this CustomerAccount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="disputeBalance" type="tns:CustomerAccountBalance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The dispute balance (total ampount of charges in dispute) for this CustomerAccount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="documentType" type="tns:CustomerAccountDocumentType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Whether the Customer receives an invoice or a bill.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="externalID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An Identifier allocated by the consumer when the entity is created. May not be updated.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="fixCollectionPolicy" type="ns2:CollectionPolicy" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The pre-determined collection policy assigned to the CustomerAccount. This will be the CollectionPolicy assigned to
						the
						CustomerAccount if it will enter collection treatment.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fixedUser" type="ns3:ApplicationUser" minOccurs="0">
				<xs:annotation>
					<xs:documentation>In special cases, the CSR may decide to assign a permanent ApplicationUser for collection treatment. This means that
						the specified ApplicationUser will handle all manual and referral activities for this CustomerAccount.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A unique identifier. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="latePaymentFeeWaiver" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If value is TRUE the CustomerAccount is waived from late payment fees.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:boolean">
						<xs:pattern value="false|true" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="manualTasks" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Manual task associated with the CustomerAccount to be manually handled.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="manualTask" type="ns6:ManualTask" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="name" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Short text, description of the CustomerAccount.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="127" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="openDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date on which the CustomerAccount was established.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="partyRoleInvolvements" minOccurs="0">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="partyRoleInvolvement" type="tns:PartyRoleCustomerAccountInvolvement" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="pastDueAmount" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An amount which should have been paid according to commitments but was not paid.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentArrangement" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If value is TRUE then the CustomerAccount is currently in payment arrangement. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:boolean">
						<xs:pattern value="false|true" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="pendingCreditBalance" type="tns:CustomerAccountBalance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The pending credit balance for this CustomerAccount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="receivableBalance" type="tns:CustomerAccountBalance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The receiveble balance for this CustomerAccount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="tns:AccountStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The status of the CustomerAccount, for example suspend, cancel.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxExemptions" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The CustomerAccountTaxExemptions associated with this CustomerAccount.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="taxExemption" type="tns:CustomerAccountTaxExemption" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="type" type="tns:CustomerAccountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Categorization of the CustomerAccount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unappliedBalance" type="tns:CustomerAccountBalance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The unapplied balance for this CustomerAccount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="writeOffAmount" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An amount of uncollected debt on which the service provider has given up expecting to collect.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="writeOffBalance" type="tns:CustomerAccountBalance" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The write-off balance for this CustomerAccount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="writeOffStatus" type="tns:WriteOffStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The status of uncollected debt on which the service provider has given up expecting to collect.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustomerInvoiceReceiver" final="extension restriction">
		<xs:annotation>
			<xs:documentation>The periodic document produced for CustomerInvoiceReceiver is CustomerInvoice, which includes the new
				AppliedCustomerBillingRates of the CustomerBillSpec. The document type is INVOICE_RECEIVER.
				:
				Derived from PIM entity
				CustomerInvoiceReceiver in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tns:CustomerBillSpec">
				<xs:sequence>
					<xs:element name="customerAccount" type="tns:CustomerAccount" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The CustomerAccount that manages this CustomerInvoiceReceiver</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="CustomerAccountBalance" final="extension restriction">
		<xs:annotation>
			<xs:documentation>Monetary amounts that reflect the results of financial transactions that were posted until a specific date.
				:
				Derived
				from PIM entity CustomerAccountBalance in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="balance" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The balance amount - can be positive or negative representing debit or credit. The balance type is determined
						according
						to the association from CustomerAccount to CustomerAccountBalance.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustomerBillingProfile" final="extension restriction">
		<xs:annotation>
			<xs:documentation>The billing specific attributes and associations for the Customer. These attributes and associations have no meaning
				for a Customer which is not instantiated in the billing system.
				:
				Derived from PIM entity CustomerBillingProfile in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="billCycleSpecification" type="ns2:CustomerBillingCycleSpecification" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The predetermined schedule according to which the Customer will be periodically billed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customer" type="ns3:Customer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The associated Customer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="cycleChangeRequest" type="tns:CycleChangeRequest" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Pending cycle change request details. CycleChangeRequests are kept in pending state until the end of the current
						cycle
						and are processed after the current cycle is closed.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dealerCode" type="ns7:Dealer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The dealer that has the contract with the Customer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="holdByOperId" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The identifier of the operator that submitted the hold request.  </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="holdIndicator" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If value is TRUE then the Customer will be excluded from billing runs when there are quality problems expected for
						this
						CustomerBillingProfile (such as the Customer uses bogus offer). This is set by the billing operator. The CSR should
						see this
						attribute in order to answer customer queries but should not be able to set this attribute. Provider write
						only.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:boolean">
						<xs:pattern value="false|true" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A unique identifier. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="largeCustomerIndicator" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If value is TRUE then the Customer is treated as a large Customer in billing processes.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:boolean">
						<xs:pattern value="false|true" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="offerCurrency" type="ns1:CurrencyCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The offer currency of the Customer. Only billing offers (PriceSchema) which match this currency are eligible for the
						Subscription or SubscriptionGroup which belong to this CustomerBillingProfile. This field is mandatory by the Amdocs
						Billing Platform.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unitID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A unique identifier for the Customer in RM Customer Hierarchy (CH) engine. This identifier can be set by the client
						system when the Customer else it is allocated by the server system. This is the node ID in RM CH system.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CycleChangeRequest" final="extension restriction">
		<xs:annotation>
			<xs:documentation>A pending request from the Customer to change the billing cycle. On the next cycle run if a request is pending, the
				invoicing process will create the bill/invoice and move the Customer to the new cycle. When this is done the request is
				complete.
				:
				Derived from PIM entity CycleChangeRequest in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="newCycleCode" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Cycle Code for the new billing cycle.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="requestDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date on which the request was issued.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="status" type="tns:ChangeCycleStatus" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Status of current request.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustomerCreditProfile" final="extension restriction">
		<xs:annotation>
			<xs:documentation>A synopsis of the credit evaluation of a Customer. It is the compound results of evaluation of references obtained from
				credit agencies and statistical information maintained by the service provider itself such as number of past due
				payments.
				:
				Derived from
				PIM entity CustomerCreditProfile in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="creditClass" type="tns:CreditClass" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The class of credit to which the Customer is entitled, for example premium, high risk, standard.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="custCreditProfileReferences" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information obtained from a credit bureau that helps define a credit worthiness.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="custCreditProfileReference" type="tns:CustCreditProfileReference" minOccurs="0" maxOccurs="unbounded" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="depositAmount" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Deposit amount required from the Customer for a post-paid subscription.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A unique identifier. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="maxNumOfSubscription" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Maximum number of post-paid subscriptions permitted for the Customer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="overridden" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether the CustomerCreditProfile was manually overridden by a user.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:boolean">
						<xs:pattern value="false|true" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="validityEnd" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date on which the credit profile ceases to be valid. No value means it is valid indefinitely.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validityStart" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date on which the credit profile was set.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CustCreditProfileReference" final="extension restriction">
		<xs:annotation>
			<xs:documentation>Information obtained from a credit bureau that helps define a credit worthiness of a Party.
				:
				Derived from PIM entity
				CustCreditProfileReference in the domain customer.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="creditAgency" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The agency which provided the reference for the Party.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="127" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="creditScore" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The score that the Party received from the credit bureau.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="id" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A unique identifier. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="referenceIdentification" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The reference identifier as provided by the credit bureau.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="validityEnd" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date on which the credit reference ceases to be valid.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validityStart" type="xs:dateTime" minOccurs="0">
