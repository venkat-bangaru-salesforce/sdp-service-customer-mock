			<xs:element name="validityEnd" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date on which the Alert became invalid or expires.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validityStart" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date on which the Alert became valid.</xs:documentation>
				</xs:annotation>
			</xs:element>
