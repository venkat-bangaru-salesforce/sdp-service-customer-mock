						<xs:maxLength value="255" />
						<xs:whiteSpace value="collapse" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="partyRole" type="ns3:PartyRole" minOccurs="0">
				<xs:annotation>
					<xs:documentation></xs:documentation>
